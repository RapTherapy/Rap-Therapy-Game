@page
@model RapGame.Pages.Frame81TemplateModel
@inject RapGame.Utils.MediaHelper mediaHelper
@{
    ViewData["Title"] = Model.MediaData.Title;
}

<div class="container animate__animated animate__fadeInUp animate__slow">
    <div class="row justify-content-center">
        <div class="col text-center display-2">
            <b>
               Well done! You have completed the rap. <br>
               I would now like you to rap along. <br>
               This time, you won’t hear my voice and you <br>
               have to try and do it to the beat.
            </b>
        </div>
    </div>

    <div class="row">
        <div class="col text-center">
            <img class="w-30" src="@mediaHelper.GetMediaPath(Model.MediaData.PathsToImages[0])"/>
        </div>
    </div>
</div>



<script src="~/js/game5.js"></script>