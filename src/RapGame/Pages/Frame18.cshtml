@page
@model RapGame.Pages.Frame18Model
@inject RapGame.Utils.MediaHelper mediaHelper
@{
    ViewData["Title"] = Model.MediaData.Title;
}
<div class="container text-center">
    <div class="row animate__animated animate__slow animate__fadeInUp">
        <div class="display-4 text-center">
            <b>Well done! <br /> Can you do it again, but this time <br /> beat the clock?</b>
        </div>

        <div>
            <img class="img w-35 preserve-width" src="@mediaHelper.GetMediaPath(Model.MediaData.PathsToImages[0])" />
        </div>
    </div>
</div>

